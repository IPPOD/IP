SELECT * FROM EMPL;




















SELECT EMPNO,EMPLOYEE FROM EMPL;
SELECT EMPLOYEE,SAL,SAL+COMM FROM EMPL;
SELECT EMPLOYEE,SAL,DEPTNO FROM EMPL WHERE COMM IS NULL;
SELECT EMPNO,EMPLOYEE,SAL,SAL*12 AS 'ANNUAL SALARY' FROM EMPL WHERE COMM IS NOT NULL;
SELECT DEPTNO FROM EMPL;
SELECT DISTINCT DEPTNO FROM EMPL;
SELECT * FROM EMPL WHERE JOB='CLERK' AND DEPTNO IS NULL;
SELECT * FROM EMPL WHERE EMPLOYEE LIKE '----';
SELECT * FROM EMPL WHERE SAL*12 BETWEEN 25000 AND 40000;
SELECT DISTINCT JOB FROM EMPL;
SELECT * FROM EMPL WHERE COMM>SAL;
SELECT EMPLOYEE,JOB,SAL FROM EMPL WHERE MGR IS NULL;
SELECT EMPLOYEE FROM EMPL WHERE EMPLOYEE LIKE '__A%';
SELECT EMPLOYEE FROM EMPL WHERE EMPLOYEE LIKE '%T';
SELECT EMPLOYEE FROM EMPL WHERE EMPLOYEE LIKE '%L%';
