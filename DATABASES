CREATE TABLE EMPL(EMPNO INT PRIMARY KEY,EMPLOYEE VARCHAR(40),JOB VARCHAR(20),MGR INT,HIRE DATE,SAL DECIMAL(10,2),COMM FLOAT(10,2),DEPTNO INT );


